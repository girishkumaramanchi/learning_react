5.Spread and Rest operators  ...

let newarray = [...oldarray,1,2]
let newobject = {...oldobject,newprop:5}

const numbers = [1,2,3];
const number = 4;
// let newnumbers = [...numbers,number];
numbers.push(number)
console.log(numbers);

const person = {
  name: "max"
}

const newperson = {
  ...person,
  age: 18
}
console.log(newperson);

const filter = (...args) => {
  return args.filter(ele => ele === 1)
}

console.log(filter(1,2,3,1,51,1,56))
